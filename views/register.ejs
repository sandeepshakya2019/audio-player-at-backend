<%- include('header'); -%>
<div class="login">
	<br>
<form class="form-signin" action="/register" method="POST" autocomplete="off">
      
      <h1 class="h3 mb-3 font-weight-normal">Signup to webdblog Audio Player</h1><br>
      
      <input type="text" id="inputName" name="name"class="form-control" placeholder="Name" required autofocus autocomplete="false"><br>
      <input type="email" id="inputEmail"name ="email" class="form-control" placeholder="Email address" required autocomplete="false"><br>
      <input type="password" id="inputPass"name ="password" class="form-control" placeholder="Password" required autocomplete="false"><br><br>
      
      <button class="btn btn-lg btn-primary btn-block" type="submit">SIGN UP</button>
      <p class="mt-5 mb-3 text-muted">Already Register ?<a href="/login"> Login</a></p>


  </form>
  <p id="errors"><%= errors %></p>
</div>
<%- include('footer'); -%>